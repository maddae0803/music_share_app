<% provide(:title, "All Users") %>

<h1> Users </h1>

<%= will_paginate %>

<ul class="users">
	<% @users.each do |user| %>
		<li>
			<!--<%= gravatar_for user %> -->
			<%= profile_pic_for user %>
			<%= link_to user.name , user %>  |  <strong> <%= user.user_type %> </strong>
		</li>
		<% end %>
</ul>

<%= will_paginate %>